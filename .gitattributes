# Auto detect text files and perform LF normalization
* text=auto
.bitmap filter=lfs diff=lfs merge=lfs -text
*.bitmap filter=lfs diff=lfs merge=lfs -text
.scenario_lightmap filter=lfs diff=lfs merge=lfs -text
.scenario_faux_data filter=lfs diff=lfs merge=lfs -text
.scenario_lightmap_bsp_data filter=lfs diff=lfs merge=lfs -text
.scenario_structure_bsp filter=lfs diff=lfs merge=lfs -text
.fsb filter=lfs diff=lfs merge=lfs -text
.tif filter=lfs diff=lfs merge=lfs -text
.wav filter=lfs diff=lfs merge=lfs -text
.tiff filter=lfs diff=lfs merge=lfs -text
.ass filter=lfs diff=lfs merge=lfs -text
